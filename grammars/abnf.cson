'scopeName': 'text.abnf.basic'
'name': 'ABNF'
'type': 'tree-sitter'
'parser': 'tree-sitter-abnf'
'fileTypes': [
  'abnf'
]
'scopes':
  'comment': 'punctuation.definition.comment'
  'rule > rulename': 'punctuation.definition.variable.bold'
  'element > rulename': 'variable'
  'repeat': 'constant.numeric'
  'char_val': 'string'
  'prose_val': 'string'
  'num_val': 'constant.numeric'
  '''
    "=",
    "=/",
    "*",
    "/",
    "[", "]",
    "(", ")"
  ''': 'keyword.control'
  '''
    "ALPHA",
    "BIT",
    "CHAR",
    "CR",
    "CRLF",
    "CTL",
    "DIGIT",
    "DQUOTE",
    "HEXDIG",
    "HTAB",
    "LF",
    "LWSP",
    "OCTET",
    "SP",
    "VCHAR",
    "WSP"
  ''': 'punctuation.definition.identity'
'folds': [
  {
    'type': 'rule'
  }
]
'comments':
  'start': '; '
